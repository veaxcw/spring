        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="main.mybatis.chapter5Mapper.DAO.MaleHealthFormDAO">

    <select id="getMaleHealthForm" parameterType="Integer" resultType="main.mybatis.chapter5Mapper.pojo.MaleHealthForm">
