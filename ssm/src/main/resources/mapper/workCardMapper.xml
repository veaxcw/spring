<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="main.mybatis.chapter5Mapper.DAO.WorkCardDAO">
    <select id="getWorkCardByEmpId" resultType="main.mybatis.chapter5Mapper.pojo.WorkCard">
        select id,emp_id as empId,real_name as realName,department,mobile,position,note from t_work_card where id = #{empId}
    </select>

</mapper>