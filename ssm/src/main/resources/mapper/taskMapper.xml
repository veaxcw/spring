        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="main.mybatis.chapter5Mapper.DAO.TaskDAO">
    <select id="getTask" parameterType="Integer" resultType="main.mybatis.chapter5Mapper.pojo.Task">
